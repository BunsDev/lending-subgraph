{
  "name": "lending-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/traderjoe_xyz/lending-subgraph",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/ subgraph.yaml",
    "codegen:rinkeby": "graph codegen --output-dir src/types/ subgraph-rinkeby.yaml",
    "build": "graph build --ipfs https://api.staging.thegraph.com/ipfs/ subgraph.yaml",
    "build:rinkeby": "graph build --ipfs https://api.staging.thegraph.com/ipfs/ subgraph-rinkeby.yaml",
    "deploy": "graph deploy traderjoe-xyz/lending --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ subgraph.yaml",
    "deploy:rinkeby": "graph deploy traderjoe-xyz/lending-rinkeby --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ subgraph-rinkeby.yaml",
    "prettier": "./node_modules/.bin/prettier â€”-write '**/*.ts'"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.18.0",
    "@graphprotocol/graph-ts": "0.18.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write --config ./.prettierrc",
      "git add"
    ]
  },
  "dependencies": {
    "@traderjoe-xyz/lending": "^0.0.3"
  }
}
